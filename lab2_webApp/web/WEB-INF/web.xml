<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>pierwszy</servlet-name>
        <servlet-class>Pierwszy</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>pierwszy</servlet-name>
        <url-pattern>/pierwszy</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>fiveNumbers</servlet-name>
        <servlet-class>FiveNumbers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>fiveNumbers</servlet-name>
        <url-pattern>/fiveNumbers</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>anyNumberPost</servlet-name>
        <servlet-class>AnyNumberPost</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>anyNumberPost</servlet-name>
        <url-pattern>/anyNumberPost</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>beerChoice</servlet-name>
        <servlet-class>com.beer.BeerChoice</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>beerChoice</servlet-name>
        <url-pattern>/beerChoice</url-pattern>
    </servlet-mapping>

</web-app>